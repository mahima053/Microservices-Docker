services:
  db:
    container_name: db
    environment:
      ACCEPT_EULA: "Y"
      MSSQL_PID: Developer
      SA_PASSWORD: Your_password123
    image: mcr.microsoft.com/mssql/server:2019-latest
    ports:
    - published: 1433
      target: 1433
  ocelotapigateway:
    build:
      context: C:\Users\thinksysuser\source\repos\WebApplication1
      dockerfile: OcelotApiGateway/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: OcelotApiGateway
      target: base
    container_name: gateway
    depends_on:
      db:
        condition: service_started
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: ocelotapigateway:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/netcoreapp3.1/OcelotApiGateway.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 5000
      target: 80
    - target: 80
    - published: 5000
      target: 443
    - target: 443
    tty: true
    volumes:
    - C:\Users\thinksysuser\source\repos\WebApplication1\OcelotApiGateway:/app:rw
    - C:\Users\thinksysuser\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\thinksysuser\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\thinksysuser\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Users\thinksysuser\.nuget\packages:/root/.nuget/packages:ro
    - C:\Users\thinksysuser\source\repos\WebApplication1:/src:rw
  webapplication1:
    build:
      context: C:\Users\thinksysuser\source\repos\WebApplication1
      dockerfile: WebApplication1/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: WebApplication1
      target: base
    container_name: customer
    depends_on:
      db:
        condition: service_started
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: webapplication1:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/netcoreapp3.1/WebApplication1.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 5000
      target: 80
    - target: 80
    - published: 5000
      target: 443
    - target: 443
    tty: true
    volumes:
    - C:\Users\thinksysuser\source\repos\WebApplication1\WebApplication1:/app:rw
    - C:\Users\thinksysuser\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\thinksysuser\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\thinksysuser\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Users\thinksysuser\.nuget\packages:/root/.nuget/packages:ro
    - C:\Users\thinksysuser\source\repos\WebApplication1:/src:rw
version: '3.4'
volumes:
  volume: {}
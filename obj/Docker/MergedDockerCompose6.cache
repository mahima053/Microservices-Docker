services:
  db:
    container_name: db
    environment:
      ACCEPT_EULA: "Y"
      MSSQL_PID: Developer
      SA_PASSWORD: Your_password123
    image: mcr.microsoft.com/mssql/server:2019-latest
    ports:
    - published: 1433
      target: 1433
  ocelotapigateway:
    build:
      context: C:\Users\thinksysuser\source\repos\WebApplication1
      dockerfile: OcelotApiGateway/Dockerfile
    container_name: gateway
    depends_on:
      db:
        condition: service_started
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:443;http://+:80
    image: ocelotapigateway
    ports:
    - target: 80
    - published: 5000
      target: 443
    - target: 443
    volumes:
    - C:\Users\thinksysuser\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\thinksysuser\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
  webapplication1:
    build:
      context: C:\Users\thinksysuser\source\repos\WebApplication1
      dockerfile: WebApplication1/Dockerfile
    container_name: customer
    depends_on:
      db:
        condition: service_started
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:443;http://+:80
    ports:
    - published: 5000
      target: 80
    - target: 80
    - published: 5000
      target: 443
    - target: 443
    volumes:
    - C:\Users\thinksysuser\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\thinksysuser\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
version: '3.4'
volumes:
  volume: {}
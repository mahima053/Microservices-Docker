{
    "Routes": [
        {
            "DownstreamPathTemplate": "/api/customer",
            "DownstreamScheme": "http",
            "DownstreamHostAndPorts": [
                {
                    "Host": "customer",
                    "Port": 5000
                }
            ],
            "UpstreamPathTemplate": "/api/customer",
            "UpstreamHttpMethod": [ "GET"],
            "“RateLimitOptions”": {
                "“ClientWhitelist”": [],
                "“EnableRateLimiting”": true,
                "“Period”": "“5s”",
                "“PeriodTimespan”": 1,
                "“Limit”": 1
            }
        }
    ],
    "GlobalConfiguration": {
        "BaseUrl": "http://localhost:5000"
    }
}